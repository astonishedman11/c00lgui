-- StarterGui.ScreenGui.Frame.2.TextButton.LocalScript
local function C_9()
local script = G2L["9"];
	local button = script.Parent
	local replicatedStorage = game:GetService("ReplicatedStorage")
	local lighting = game:GetService("Lighting")
	
	-- Create the RemoteEvent in ReplicatedStorage
	local event = replicatedStorage:FindFirstChild("ChangeSkyEvent")
	
	if not event then
		event = Instance.new("RemoteEvent")
		event.Name = "ChangeSkyEvent"
		event.Parent = replicatedStorage
	end
	
	-- Function to change the sky when button is clicked
	local function changeSky()
		-- Check if a Sky object exists in Lighting
		local sky = lighting:FindFirstChildOfClass("Sky")
	
		if not sky then
			-- Create a new Sky if none exists
			sky = Instance.new("Sky")
			sky.Parent = lighting
		end
	
		-- Change the Skybox (replace with real asset IDs)
		sky.SkyboxUp = "rbxassetid://10560525674"
		sky.SkyboxDn = "rbxassetid://10560525674"
		sky.SkyboxLf = "rbxassetid://10560525674"
		sky.SkyboxRt = "rbxassetid://10560525674"
		sky.SkyboxBk = "rbxassetid://10560525674"
		sky.SkyboxFt = "rbxassetid://10560525674"
	
		print("Sky has been changed!")
	end
	
	-- Connect button click to sky change
	button.MouseButton1Click:Connect(changeSky)
	
end;
task.spawn(C_9);
