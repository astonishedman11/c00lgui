-- StarterGui.ScreenGui.Frame.TextButton.LocalScript
local function C_1a()
local script = G2L["1a"];
	local player = game.Players.LocalPlayer
	local button = script.Parent
	button.MouseButton1Click:Connect(function()
		if player.Character then
			for _, part in pairs(player.Character:GetChildren()) do
				if part:IsA("BasePart") and part.Name ~= "HumanoidRootPart" then
					part.Transparency = 0.8 -- Semi-invisible
					part.CanCollide = false -- Optional: Prevent blocking
					print("ez")
				end
			end
		end
	end)
	local button = script.Parent
	local player = game:GetService("Players").LocalPlayer
	local replicatedStorage = game:GetService("ReplicatedStorage")
	
	-- Check if the RemoteEvent exists; if not, create it
	local event = replicatedStorage:FindFirstChild("MakeInvisibleEvent")
	
	if not event then
		event = Instance.new("RemoteEvent")
		event.Name = "MakeInvisibleEvent"
		event.Parent = replicatedStorage
	end
	
	-- When button is clicked, fire the event to the server
	button.MouseButton1Click:Connect(function()
		event:FireServer()
		print("Sent invisibility request to server")
	end)
	
end;
task.spawn(C_1a);
-- StarterGui.ScreenGui.Frame.TextButton.LocalScript
local function C_1c()
local script = G2L["1c"];
	local button = script.Parent -- The GUI button
	local player = game.Players.LocalPlayer
	local gui = player:WaitForChild("PlayerGui")
	
	-- Function to show the fullscreen decal
	local function createDecal()
		-- Check if decal already exists, remove it if so
		local existingDecal = gui:FindFirstChild("FullscreenDecal")
		if existingDecal then
			existingDecal:Destroy()
		end
	
		-- Create a new fullscreen ImageLabel
		local imageFrame = Instance.new("ImageLabel")
		imageFrame.Name = "FullscreenDecal"
		imageFrame.Size = UDim2.new(1, 0, 1, 0) -- Fullscreen
		imageFrame.Position = UDim2.new(0, 0, 0, 0)
		imageFrame.BackgroundTransparency = 1 -- No background
		imageFrame.Image = "rbxassetid://10560525674" -- Replace with your decal ID
		imageFrame.ScaleType = Enum.ScaleType.Stretch -- Ensures it stretches to fill the screen
		imageFrame.Parent = gui
		print("click")
	
		-- Wait 3 seconds, then remove the decal
		task.wait(3)
		if imageFrame then
			imageFrame:Destroy()
		end
	end
	
	-- Connect button click event
	button.MouseButton1Click:Connect(createDecal)
	
	
end;
task.spawn(C_1c);

return G2L["1"], require;
