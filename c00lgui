local function C_18()
local script = G2L["18"];
	local button = script.Parent
	
	-- Create and configure the sound
	local sound = Instance.new("Sound", button)
	sound.SoundId = "rbxassetid://1846924681" -- Sound ID (Button Click)
	sound.Volume = 1
	sound.PlayOnRemove = true 
	sound.Looped=true -- Instantly plays when removed
	
	-- Connect the button click to the sound
	button.MouseButton1Click:Connect(function()
		sound:Play()
	end)
	
