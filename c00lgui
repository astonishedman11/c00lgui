-- StarterGui.ScreenGui.Frame.TextButton.LocalScript
local function C_14()
local script = G2L["14"];
	local button = script.Parent
	local imageId = "rbxassetid://10560525674" -- Replace with your Decal ID
	local isActive = false
	
	local faces = {
		Enum.NormalId.Front,
		Enum.NormalId.Back,
		Enum.NormalId.Left,
		Enum.NormalId.Right,
		Enum.NormalId.Top,
		Enum.NormalId.Bottom
	}
	
	-- Function to apply decals to all parts
	local function applyDecals()
		for _, part in pairs(workspace:GetDescendants()) do
			if part:IsA("BasePart") and not part:IsA("Terrain") then
				for _, face in ipairs(faces) do
					local decal = Instance.new("Decal")
					decal.Texture = imageId
					decal.Face = face
					decal.Parent = part
				end
			end
		end
		button.Text = "Remove Decals"
	end
	
	-- Function to remove all decals
	local function removeDecals()
		for _, obj in pairs(workspace:GetDescendants()) do
			if obj:IsA("Decal") then
				obj:Destroy()
			end
		end
		button.Text = "Apply Decals"
	end
	
	button.MouseButton1Click:Connect(function()
		if isActive then
			removeDecals()
		else
			applyDecals()
		end
		isActive = not isActive
	end)
	
end;
task.spawn(C_14);
